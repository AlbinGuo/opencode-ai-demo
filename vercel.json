{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1",
      "headers": {
        "Cache-Control": "s-maxage=10, stale-while-revalidate"
      }
    }
  ],
  "outputDirectory": "frontend/dist"
}