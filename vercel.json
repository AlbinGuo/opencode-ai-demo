{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vue",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api-proxy/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api-proxy/$1"
    },
    {
      "source": "/((?!api|auth).*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}
